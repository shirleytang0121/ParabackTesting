{
  "name": "parashop_backend",
  "version": "1.0.0",
  "description": "mobile AR app that encourages people with ASD to shop",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec babel-node src",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "jest",
    "coveralls": "npm run test --coverage --watchAll=false && cat ./coverage/lcov.info | coveralls",
    "build": "babel src -d dist",
    "start": "npm run build && node dist/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "Nan Chen",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-validator": "^6.3.0",
    "request-promise": "^4.2.5",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2"
  }
}
